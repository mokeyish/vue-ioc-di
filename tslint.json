{
    "extends": [
        "tslint:recommended"
    ],
    "defaultSeverity": "error",
    "jsRules": {},
    "rules": {
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock",
            "check-postbrace"

        ],
        "quotemark": [
            true,
            "single"
        ],
        "no-empty": false,
        "no-empty-interface": false,
        "no-bitwise": false,
        "ban-types": false,
        "max-classes-per-file": false,
        "unified-signatures": false,
        "no-unused-expression": false,
        "no-shadowed-variable": false,
        "only-arrow-functions": false,
        "prefer-for-of": false,
        "no-console": false
    },
    "rulesDirectory": [ ]
}
